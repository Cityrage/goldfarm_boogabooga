local HWIDTable = loadstring(game:HttpGet("https://raw.githubusercontent.com/iamsillycheater/farm1/refs/heads/main/hwidwhitelist"))()
local HWID = game:GetService("RbxAnalyticsService"):GetClientId()
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Whitelist | GoldFarm", HidePremium = true, SaveConfig = false})

OrionLib:MakeNotification({
	Name = "Loading",
	Content = "Make sure to get whitelisted",
	Image = "rbxassetid://4483345998",
	Time = 5
})


local Tab = Window:MakeTab({
	Name = "Whitelist",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


Tab:AddButton({
	Name = "Get Hwid",
	Callback = function()
      	OrionLib:MakeNotification({
        	Name = "Your HWID is",
	        Content = ""..HWID.."",
	        Image = "rbxassetid://4483345998",
	        Time = 5
        })
  	end    
})

Tab:AddButton({
	Name = "Load Script",
	Callback = function()
      	for i,v in pairs(HWIDTable) do
	        if v == HWID then
			    OrionLib:Destroy()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/iamsillycheater/farm1/refs/heads/main/main"))()
			else
			    OrionLib:MakeNotification({
	                Name = "Error",
	                Content = "You are not whitelisted",
	                Image = "rbxassetid://4483345998",
	                Time = 5
                })
			end
		end		
	end
})


OrionLib:Init()


